package {	import flash.events.Event;	import flash.display.MovieClip;	import flash.display.MovieClip;	public class EBomb extends Enemy{		private var timer:int = 0;		private var zap:Boolean;		public function EBomb (a:Attractable, b:Attractable, zappy:Boolean = false){			super(a, b, false);			timer = 0;			handle.friction = .9;			zap = zappy;			handle.rumble = 20;		}		public override function update(){						timer++;			if(timer > 100 && !zap){				handle.die();				handle.points = 0;				var speed:Vec2D = new Vec2D(0, -1);				speed.length = 5;				if(handle.clip.scaleX<.9){					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(90);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(90);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(90);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);				} else {					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);					speed.rotate(45);					ROOT.launchMissile(new Vec2D(handle.x, handle.y), speed);				}			}						if(timer == 200 && zap){				handle.points = 0;				ROOT.addTractor(handle, base, 0xFF0000);			}						handle.clip.rotation += Vec2D.angle2(new Vec2D(0, 1).rotated(handle.clip.rotation), new Vec2D(base.x - handle.x, base.y-handle.y)) * .1;		}	}}