package  {		public class EB2Orbiter extends Enemy{		public var timer:Number;		private var targ:Attractable;		private var bar:Vec2D;		public function EB2Orbiter (a:Attractable, b:Attractable, tgt:Attractable){			super(a, b, false);			timer = -10;			targ = tgt;			bar = new Vec2D(handle.x-targ.x, handle.y-targ.y);			handle.momentum.x = -bar.y/bar.length*2/handle.invmass;			handle.momentum.y = bar.x/bar.length*2/handle.invmass;		}		public override function update(){			if(timer >= 1) targ = base;			var dist:Vec2D = new Vec2D(targ.y-handle.y, -(targ.x - handle.x));			dist.length = 3;			timer += .050;;									if(timer >= 1){				var dir:Vec2D = new Vec2D(dist.x / (timer>=1?timer*3:1) - dist.y * (timer>=1?timer*3:.5),										  dist.y / (timer>=1?timer*3:1) + dist.x * (timer>=1?timer*3:.5))				dir.norm();								handle.momentum.x += dir.x * 5;				handle.momentum.y += dir.y * 5;								handle.clip.rotation += Vec2D.angle2(new Vec2D(0, -1).rotated(handle.clip.rotation), handle.momentum);			} else {				bar.rotate(5);				handle.momentum.x += (bar.x+targ.x - handle.x);				handle.momentum.y += (bar.y+targ.y - handle.y);								handle.clip.rotation += Vec2D.angle2(new Vec2D(0, -1).rotated(handle.clip.rotation), bar.perpendicular());			}		}	}	}