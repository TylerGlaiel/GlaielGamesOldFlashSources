package {	import flash.events.Event;	import flash.display.MovieClip;	public class ESpeeder extends Enemy{		public var timer:Number;		public function ESpeeder (a:Attractable, b:Attractable){			super(a, b);			timer = -10;		}		public override function update(){			var dist:Vec2D = new Vec2D(base.y-handle.y, -(base.x - handle.x));			dist.length = 3;			timer += .020/Cam.scale;						var dir:Vec2D = new Vec2D(dist.x / (timer>=1?timer*3:1) - dist.y * (timer>=1?timer*3:.4),									  dist.y / (timer>=1?timer*3:1) + dist.x * (timer>=1?timer*3:.4))			dir.norm();						handle.momentum.x += dir.x * 5;			handle.momentum.y += dir.y * 5;						handle.clip.rotation = Vec2D.angle2(new Vec2D(0, -1), handle.momentum);		}	}}