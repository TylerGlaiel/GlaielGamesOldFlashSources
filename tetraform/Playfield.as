package  {		public class Playfield {		public static const width:Number = 1400*Cam.scale;		public static const height:Number = 1400*Cam.scale;		public static const x:Number = 350;		public static const y:Number = 300;				public static function get xmin():Number{ return x - width/2; }		public static function get xmax():Number{ return x + width/2; }		public static function get ymin():Number{ return y - height/2; }		public static function get ymax():Number{ return y + height/2; }	}	}