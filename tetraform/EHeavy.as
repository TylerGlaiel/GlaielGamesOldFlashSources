package {	import flash.events.Event;	import flash.display.MovieClip;	public class EHeavy extends Enemy{		public var timer:Number;		public function EHeavy (a:Attractable, b:Attractable){			super(a, b);			timer = -10;			handle.rumble = 15;		}		public override function update(){			var dist:Vec2D = new Vec2D(base.y-handle.y, -(base.x - handle.x));			dist.length = 3;			timer += .0125/Cam.scale;						var dir:Vec2D = new Vec2D(dist.x / (timer>=1?timer*3:1) - dist.y * (timer>=1?timer*3:.4),									  dist.y / (timer>=1?timer*3:1) + dist.x * (timer>=1?timer*3:.4))			dir.norm();						handle.momentum.x += dir.x * 20;			handle.momentum.y += dir.y * 20;						var dist2:Vec2D = new Vec2D(base.x-handle.x, base.y-handle.y);			if(dist2.length > 1050){				dist2.norm();				handle.momentum.x += dist2.x * 20;			    handle.momentum.y += dist2.y * 20;			}						handle.clip.rotation = Vec2D.angle2(new Vec2D(0, -1), handle.momentum);		}	}}