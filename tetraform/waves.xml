<?xml version="1.0" encoding="UTF-8" ?><!--tags:    wave: describes the formations appearing in the wave            waves will end when all items in it have been destroyed / collected or            when timeout rins out          timeout: how long to wait before spawning next wave after this wave is complete, in frames.              suggested: 900 for normal waves                         -1 for bosses (no timeout)                         150 for powerup waves          name: name for the wave                                   all waves have a default 150 frame pause after timeout or beat anyway                                  group: group of enemies comes in a shuffled order,              next group begins immediately after one group is done spawning          freq: how many frames to wait between spawning enemies. lower = faster spawning              cycle: same as group except spawning repeats until previous group is killed          repeat: # of times to repeat (0 for indefinate)          time: how many frames to wait before repeating          freq: how many frames to wait before spawning enemies              pause: N frame pause              enemy: the type of enemy to spawn and how many        type: the type of enemy        count: how many        (optional multipliers):            life: life multiplier for enemies            size: size multiplier for enemies            damage: damage multiplier for enemies                powerup: indicates how many powerups to spawn in this wave, and of what type               if powerup count > number of enemies, powerups will spawn on their own         type: powerup type (random = total random, list multiple types for random to pick from elsewise)         count: how many to spawn--><game>      <wave timeout="900" name="WAVE 1">            <group freq="50">            <enemy>                <type>Basic</type>                <count>20</count>            </enemy>        </group>            </wave>        <wave timeout="150" name="HEAL!">            <group freq="1">            <powerup>                <type>Health</type>                <count>10</count>            </powerup>        </group>            </wave>        <wave timeout="900" name="WAVE 2">            <group freq="100">            <enemy>                <type>Basic</type>                <count>20</count>            </enemy>            <enemy>                <type>Launcher</type>                <count>5</count>            </enemy>        </group>            </wave>        <wave timeout="150" name="RECOVER!">            <group freq="1">            <powerup>                <type>Health</type>                <count>5</count>            </powerup>			<powerup>                <type>Nuke</type>                <count>1</count>            </powerup>        </group>            </wave>    <wave timeout="-1" name="WAVE 3">            <group freq="25">            <enemy>                <type>Launcher</type>                <count>5</count>            </enemy>        </group>        <pause>300</pause>        <group freq="25">            <enemy>                <type>Speeder</type>                <count>30</count>            </enemy>        </group>            </wave>        <wave timeout="300" name="GRAB IT!">            <group freq="30">            <powerup>                <type>Spike</type>                <count>1</count>            </powerup>        </group>            </wave>        <wave timeout="1200" name="WAVE 4">            <group freq="100">            <enemy>                <type>Launcher</type>                <count>5</count>            </enemy>            <enemy>                <type>Heavy</type>                <count>15</count>            </enemy>        </group>            </wave>        <wave timeout="-1" name="WAVE 5">            <group freq="50">            <enemy>                <type>Launcher</type>                <count>10</count>            </enemy>            <enemy>                <type>Heavy</type>                <count>5</count>            </enemy>            <enemy>                <type>Speeder</type>                <count>15</count>            </enemy>            <enemy>                <type>Basic</type>                <count>5</count>            </enemy>        </group>            </wave>        <wave timeout="150" name="GET READY">            <group freq="1">            <powerup>                <type>Health</type>                <count>10</count>            </powerup>        </group>            </wave>        <wave timeout="-1" name="INCOMING BOSS">            <group freq="0">            <enemy>                <type>MegaBomber</type>                <count>1</count>            </enemy>        </group>            </wave>        <wave timeout="150" name="GOOD JOB!">            <group freq="1">            <powerup>                <type>Health</type>                <count>10</count>            </powerup>            <powerup>                <type>Spike</type>                <count>1</count>            </powerup>        </group>            </wave>        <wave timeout="1200" name="WAVE 6">            <group freq="25">            <enemy>                <type>Speeder</type>                <count>30</count>            </enemy>        </group>         <pause>150</pause>        <group freq="50">            <enemy>                <type>FBomber</type>                <count>30</count>            </enemy>			<powerup>                <type>Nuke</type>                <count>1</count>            </powerup>        </group>            </wave>        <wave timeout="1500" name="???">            <group freq="300">            <enemy>                <type>Phantom</type>                <count>2</count>            </enemy>        </group>            </wave>    <wave timeout="1500" name="WAVE 7">            <group freq="25">            <enemy>                <type>Phantom</type>                <count>20</count>            </enemy>			<powerup>                <type>Spike</type>                <count>1</count>            </powerup>        </group>        <pause>300</pause>        <group freq="35">            <enemy>                <type>FBomber</type>                <count>20</count>            </enemy>            <enemy>                <type>Launcher</type>                <count>8</count>            </enemy>            <enemy>                <type>Phantom</type>                <count>10</count>            </enemy>			<powerup>                <type>Nuke</type>                <count>1</count>            </powerup>        </group>                    </wave>		<wave timeout="-1" name="WAVE 8">            <group freq="25">            <enemy>                <type>FBomber</type>                <count>30</count>            </enemy>			<enemy>                <type>Speeder</type>                <count>30</count>            </enemy>        </group>                   </wave>		<wave timeout="150" name="GET READY">            <group freq="1">            <powerup>                <type>Health</type>                <count>10</count>            </powerup>			<powerup>                <type>Spike</type>                <count>1</count>            </powerup>        </group>            </wave>		<wave timeout="-1" name="INCOMING BOSS">            <group freq="0">            <enemy>                <type>StaticBomber</type>                <count>1</count>            </enemy>        </group>            </wave>		<wave timeout="150" name="GOOD JOB!">            <group freq="1">            <powerup>                <type>Health</type>                <count>10</count>            </powerup>        </group>            </wave>		<wave timeout="900" name="WAVE 9">            <group freq="25">            <enemy>                <type>Radiater</type>                <count>30</count>            </enemy>			<powerup>                <type>Nuke</type>                <count>1</count>            </powerup>        </group>		<group freq="15">            <enemy>                <type>Radiater</type>                <count>20</count>            </enemy>			<enemy>                <type>Speeder</type>                <count>20</count>            </enemy>			<enemy>                <type>FBomber</type>                <count>10</count>            </enemy>			<powerup>                <type>Spike</type>                <count>1</count>            </powerup>        </group>            </wave>		<wave timeout="1500" name="WAVE 10">            <group freq="50">            <enemy>                <type>Bomber</type>                <count>10</count>            </enemy>			<enemy>                <type>Heavy</type>                <count>10</count>            </enemy>        </group>            </wave>		<wave timeout="1500" name="WAVE 11">            <group freq="50">            <enemy>                <type>Phaser</type>                <count>20</count>            </enemy>			<enemy>                <type>Phantom</type>                <count>5</count>            </enemy>			<enemy>                <type>Radiater</type>                <count>5</count>            </enemy>        </group>            </wave>			<wave timeout="150" name="GET READY">            <group freq="1">            <powerup>                <type>Health</type>                <count>10</count>            </powerup>        </group>            </wave>		<wave timeout="-1" name="INCOMING BOSS">            <group freq="0">            <enemy>                <type>ElectroBomber</type>                <count>1</count>            </enemy>        </group>            </wave>		<wave timeout="150" name="PHEW!">            <group freq="1">            <powerup>                <type>Health</type>                <count>20</count>            </powerup>			<powerup>                <type>Spike</type>                <count>1</count>            </powerup>			<powerup>                <type>Nuke</type>                <count>1</count>            </powerup>        </group>            </wave>		<wave timeout="900" name="WAVE 12">            <group freq="25">            <enemy>                <type>Oscilator</type>                <count>30</count>            </enemy>        </group>            </wave>		<wave timeout="900" name="WAVE 13">            <group freq="50">            <enemy>                <type>Basic</type>                <count>8</count>            </enemy>			<enemy>                <type>Launcher</type>                <count>8</count>            </enemy>			<enemy>                <type>Heavy</type>                <count>5</count>            </enemy>        </group>		<group freq="30">            <enemy>                <type>Bomber</type>                <count>10</count>            </enemy>			<powerup>                <type>Nuke</type>                <count>1</count>            </powerup>        </group>            </wave>		<wave timeout="900" name="WAVE 14">            <group freq="50">            <enemy>                <type>Phantom</type>                <count>15</count>            </enemy>			<enemy>                <type>Phaser</type>                <count>15</count>            </enemy>        </group>            </wave>		<wave timeout="900" name="WAVE 15">            <group freq="25">            <enemy>                <type>Speeder</type>                <count>20</count>            </enemy>			<enemy>                <type>FBomber</type>                <count>20</count>            </enemy>			<enemy>                <type>Radiater</type>                <count>20</count>            </enemy>			<enemy>                <type>Oscilator</type>                <count>20</count>            </enemy>        </group>		<pause>400</pause>		<group freq="10">            <enemy>                <type>Speeder</type>                <count>20</count>            </enemy>			<enemy>                <type>FBomber</type>                <count>20</count>            </enemy>			<enemy>                <type>Radiater</type>                <count>20</count>            </enemy>			<enemy>                <type>Oscilator</type>                <count>20</count>            </enemy>			<powerup>				<type>Spike</type>				<count>1</count>			</powerup>        </group>            </wave>    	<wave timeout="150" name="GET READY">            <group freq="1">            <powerup>                <type>Health</type>                <count>20</count>            </powerup>        </group>            </wave>		<wave timeout="-1" name="FINAL BOSS!">            <group freq="0">            <enemy>                <type>WarpBomber</type>                <count>1</count>            </enemy>        </group>            </wave>	</game>