class Cam {	public var pos:Vector;	public var xMin:Number;	public var yMin:Number;	public var xMax:Number;	public var yMax:Number;	public function Cam() {		pos = new Vector(0, 0);		xMin = 0;		yMin = 0;		xMax = 0;		yMax = 0;	}	public function setUp(position:Vector, minx:Number, miny:Number, maxx:Number, maxy:Number):Void {		pos.x = position.x;		pos.y = position.y;		xMin = minx;		yMin = miny;		xMax = maxx;		yMax = maxy;	}	public function update():Void {		pos.x<xMin ? pos.x=xMin : 0;		pos.x>xMax ? pos.x=xMax : 0;		pos.y<yMin ? pos.y=yMin : 0;		pos.y>yMax ? pos.y=yMax : 0;		_root._x = 275-pos.x;		_root._y = 200-pos.y;	}}