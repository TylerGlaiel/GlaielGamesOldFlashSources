class Container {	private var dat:Array;	public var length:Number;	public function Container() {		dat = new Array();		length = 0;	}	public function add(obj):Void {		dat.push(obj);		length += 1;	}	public function remove(obj):Void {		var spot:Number = -1;		for (var i:Number = 0; i<length; i++) {			if (dat[i] == obj) {				spot = i;				i = length+1;			}		}		if (spot != -1) {			dat.splice(spot, 1);			length -= 1;		}	}	public function move(obj, newContainer:Container):Void {		remove(obj);		newContainer.add(obj);	}	public function getItem(index:Number) {		return (dat[index]);	}	public function clear() {		dat.splice(0, length);		length = 0;	}}